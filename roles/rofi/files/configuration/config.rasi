configuration {
  modi: "drun,calc,clipboard:greenclip print";
  sidebar-mode: true;
  scroll-method: 1;
  lines: 10;
  fixed-num-lines: false;
  show-icons: true;
  normal-window: true;

  kb-remove-to-eol: "";
  kb-remove-char-back: "BackSpace";
  kb-mode-next: "Tab";
  kb-mode-previous: "Shift+Tab";
  kb-row-tab: "";
}

@theme "themes/custom.rasi"

